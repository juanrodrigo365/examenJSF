    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>/AcercaDe.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>inicio1</from-outcome>
    		<to-view-id>/inicio.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    
    <navigation-rule>
    	<from-view-id>/AcercaDe.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>contactos1</from-outcome>
    		<to-view-id>/contactos.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>/contactos.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>acerca1</from-outcome>
    		<to-view-id>/AcercaDe.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    
</faces-config>
